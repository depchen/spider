class JobBoleArticleItem(scrapy.Item):
    title=scrapy.Field()#标题
    create_time=scrapy.Field()#创建时间
    url=scrapy.Field()#url
    url_object_id=scrapy.Field()#url md5
    front_image_url=scrapy.Field()#列表图片url
    front_path_url = scrapy.Field()#本地图片url
    praise_num=scrapy.Field()#点赞数
    comment_num = scrapy.Field()#评论数
    fav_num = scrapy.Field()#收藏数
    tags=scrapy.Field()#标签
    content=scrapy.Field()#内容